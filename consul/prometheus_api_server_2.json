{
  "service": {
    "name": "prometheus-api-server-2",
    "tags": [
      "prometheus-api-server",
      "urlprefix-/metrics",
      "urlprefix-/prometheus-api-server-2:8080/metrics"
    ],
    "port": 8080,
    "check": {
      "args": [
        "curl",
        "prometheus-api-server-2:8080/__health"
      ],
      "interval": "10s"
    }
  }
}